{
    "name": "Vue DataGrid",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "vue-dev",
	"workspaceFolder": "/var/www/html",
	"customizations": {
		"vscode": {
			"settings": {
				"[vue]": {
					"editor.tabSize": 2,
					"editor.insertSpaces": true
				},
				"[php]": {
					"editor.tabSize": 4,
					"editor.insertSpaces": true
				},
				"[javascript]": {
					"editor.tabSize": 2,
					"editor.insertSpaces": true
				},
				"files.associations": {
					"*.css": "tailwindcss"
				}
			},
			"extensions": [
				"mhutchie.git-graph",
				"eamodio.gitlens",
				"PKief.material-icon-theme",
				"donjayamanne.githistory",
				"vue.volar",
				"bradlc.vscode-tailwindcss"
			]
		}
	}
}